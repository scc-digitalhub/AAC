{
  "name": "AAC",
  "version": "6.0.0",
  "private": true,
  "scripts": {
    "prettier:application": "prettier --with-node-modules --plugin=prettier-plugin-java --config ./.prettierrc.yml --write --list-different \"application/src/**/*.java\"",
    "prettier:modules": "prettier --with-node-modules --plugin=prettier-plugin-java --config ./.prettierrc.yml --write --list-different \"modules/src/**/*.java\" \"idps/src/**/*.java\"",
    "prettier": "prettier --with-node-modules --plugin=prettier-plugin-java --config ./.prettierrc.yml --write --list-different \"modules/*/src/**/*.java\" \"idps/*/src/**/*.java\" \"frontend/*/src/**/*.java\" \"application/src/**/*.java\""
  },
  "devDependencies": {
    "prettier": "3.2.5",
    "prettier-plugin-java": "2.6.0"
  },
  "dependencies": {},
  "eslintConfig": {}
}
